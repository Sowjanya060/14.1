#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    while(t--)
    {
        long int n,i,sum=0;
        scanf("%ld",&n);
        long int a[n];
        for(i=0;i<n;i++)
        scanf("%ld",&a[i]);
        for(i=0;i<n;i++)
        sum=sum+a[i];
        for(i=0;i<n;i++)
        printf("%ld \n",sum-a[i]);
    }
	// your code goes here
	return 0;
}

